spring.datasource.url=jdbc\:mysql\://\:3306/test?useUnicode\=true&characterEncoding\=gbk&zeroDateTimeBehavior\=convertToNull
spring.datasource.username=act_web
spring.datasource.password=act_web
spring.datasource.driver-class-name=com.mysql.jdbc.Driver

spring.datasource.type=com.alibaba.druid.pool.DruidDataSource
# \u521D\u59CB\u5316\u5927\u5C0F\uFF0C\u6700\u5C0F\uFF0C\u6700\u5927
spring.datasource.initialSize=5  
spring.datasource.minIdle=5  
spring.datasource.maxActive=20  
spring.datasource.maxWait=60000 


#数据库监控配置
management.health.db.enabled = true
management.health.defaults.enabled= true

#应用磁盘空间检查配置
management.health.diskspace.enabled=true
management.health.diskspace.path= D:/
management.health.diskspace.thresholed=0

management.health.redis.enabled = true
management.health,status.order=DOWN,OUT_OF_SERVICE,UNKONW,UP